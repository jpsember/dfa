# An anonymous token, for convenience; a non-empty sequence of digits
#
_DIG: \d+


WS: $_WHITESPACE

# Double has higher priority than int, since we don't want the prefix of the double
# to be intpreted as an int

INT: \-?$_DIG

DBL: \-?([0] | ([1-9]\d*)) . \d+

LBL: '([^'\n]|\\')*'

ID:  [_a-zA-Z][_a-zA-Z0-9]*

ASSIGN: =

EQUIV: ==

IF: if

DO: do

BROP: \{

BRCL: \}

TILDE: _TILDE_

